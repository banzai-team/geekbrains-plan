datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// generator
generator client {
  provider             = "prisma-client-py"
  interface            = "asyncio"
}

model ModelRequest {
    id              Int     @id @default(autoincrement())
    request            String
    performed_at       DateTime
}

model ModelResponse {
    id Int @id @default(autoincrement())
    request_id Int
    started_at      DateTime?
    finished_at     DateTime?
}